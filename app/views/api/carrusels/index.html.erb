<p id="notice"><%= notice %></p>

<h1>Api Carrusels</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_carrusels.each do |api_carrusel| %>
      <tr>
        <td><%= link_to 'Show', api_carrusel %></td>
        <td><%= link_to 'Edit', edit_api_carrusel_path(api_carrusel) %></td>
